def fun():
    for i in range(1,4):
        for j in range(1,11):
            if (i==j):
                pass
            print(f"{i}*{j}={i*j}\t\t\n")
    print()
    
    
fun()


txt = "Hello my FRIENDS"

x = txt.lower()

print(x)


for i in range (1, 6):
    for j in range (1,i+1):
        print("*", end = " ")
    print("")
    
for i in range (1, 6):
    print("* ")
    for j in range (1,i+1):
        print("")
    
    
